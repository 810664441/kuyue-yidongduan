    display: block;
    height: .7rem;
    line-height: .7rem;
    overflow: hidden;
    text-align: center;
    font-size: .28rem;
}
section.right ul li a{
    display: block;
    font-size: .24rem;
    text-align: center;
}
section.right ul li img{
