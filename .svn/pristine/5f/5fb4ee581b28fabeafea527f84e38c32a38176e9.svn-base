nav.left{
    width: 1.87rem;
    border:none;
    float:left;
    overflow-y: scroll;
}
nav.left::-webkit-scrollbar {display:none}
nav.left .list li{
    list-style: none;
    width: 1.87rem; 
    height: .9rem;
    padding: .26rem 0;
    font-size: .28rem;
    text-align: center;
    color:#333;
    background-color:#F6F6F9;
}
nav.left .list li.active{
    background-color:white;
}
/*公用部分*/
section.right  a:link{
    color:#333;
}
section.right  a:visited{
    color:#333;
}
section.right  a:hover{
    color:#333;
}
/*公用部分*/
section.right  a:active{
    color:#333;
}
div.bottom{
    background-color:white;
}
